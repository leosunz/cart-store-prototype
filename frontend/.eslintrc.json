{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript"
    ],
    "parser": "@typescript-eslint/parser",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json",
        "extraFileExtensions:": [".html", ".scss"]
    },
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "rules": {
    },
    "prettier/prettier": [
        2,
        {
          "arrowParens": "avoid",
          "singleQuote": true,
          "bracketSpacing": true,
          "tabWidth": 2,
          "useTabs": false,
          "endOfLine": "auto"
        }
      ]
}
